  "description": "MCP server for Phosphor Icons - provides access to beautiful SVG icons with multiple weights",
  "type": "module",
  "main": "dist/index.js",
  "module": "./src/index.ts",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
